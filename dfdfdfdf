local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Key", "DarkTheme")
local Key = Window:NewTab("Key")

_G.Key = "Beluga_Chocolate7788!"
_G.KeyInput = "string"

function MakeScriptHub()
    loadstring(game:httpget('https://raw.githubusercontent.com/JSPSSS/ssssssssssss/main/ssssss'))()
end

KeySection:NewTextBox("Key here", "Key", function(txt)
	_G.KeyInput = txt
end)


KeySection:NewButton("Check Key", "check key", function()
    if _G.KeyInput == _G.Key then
       MakeScriptHub() 

    end
end)
